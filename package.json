  "scripts": {
    "start": "webpack-dev-server --config webpack.config.development.js",
    "clearcache": "rimraf node_modules/.cache",
    "prebuild": "rimraf build",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.production.js",
    "build-dll": "webpack --config webpack.config.dll.js",
    "prepare": "yarn clearcache && yarn prebuild && yarn build-dll && yarn build",
    "export": "gh-pages -d build",
    "deploy": "yarn prepare && yarn export && yarn prebuild"
  }
